class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        pairs = {')': '(',
                    '}': '{',
                    ']': '['}
        opening = ['(', '{', '[']
        closing = [')', ']', '}']
        stack = Stack()
        for i in range(len(s)):
            if (s[i] in opening):
                stack.push(s[i])
            if(s[i] in closing):
                if stack.isEmpty():
                    return False
                else:    
                    if(stack.peek() == pairs[s[i]]):
                        stack.pop()
                    else:
                        return False
        return stack.isEmpty()
        
class Stack:
    def __init__(self):
        self.items = []

    def push(self, item):
        self.items.append(item)

    def pop(self):
        return self.items.pop() if self.items else None

    def peek(self):
        return self.items[-1] if self.items else None

    def isEmpty(self):
        return not self.items

    def size(self):
        return len(self.items)
